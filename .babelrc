{
  "presets": [
    ["es2015","stage-0"]
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    ["component", [
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-default"
      }
    ]]
  ],
  "ignore":[
    "vue.min.js",
    "vue-resource.es2015.js",
    "vue.common.min.js"
  ]
}